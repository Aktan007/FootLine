[MASTER]
ignore=migrations

[MESSAGES CONTROL]
disable=
    no-member,
    import-error,
    unsupported-binary-operation,
    import-outside-toplevel,
    too-few-public-methods,
    missing-module-docstring,
    missing-class-docstring,
    missing-function-docstring,
    invalid-name,
    trailing-whitespace,
    invalid-str-returned,
    too-many-locals

[FORMAT]
max-line-length=120
